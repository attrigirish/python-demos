#Getter and Setter Functions

class Student:
    roll=0
    name=''
    course=''
    fees=0.0

    def Input(self,r,n,c,f):
        self.roll=r
        self.name=n
        self.course=c
        self.fees=f
    
    def SetName(self,n):
        self.name=n

    def SetCourse(self,c):
        self.course=c

    def SetFees(self,f):
        self.fees=f

    def GetRoll(self):
        return self.roll

    def GetName(self):
        return self.name

    def GetCourse(self):
        return self.course

    def GetFees(self):
        return self.fees




s=Student()

i=input("Enter Id : ")

s.Input(i,'Rishabh','ACCP',43000)

print("Roll : ", s.GetRoll())
print("Name : ", s.GetName())
print("Course : ",s.GetCourse())
print("Fees : ",s.GetFees())
